    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^22.2.3",
    "@types/react": "~16.3.2",
    "@types/react-dom": "^16.0.4",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.2",
    "enzyme": "^3.3.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.4.0",

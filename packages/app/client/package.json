{
  "name": "@bfemulator/client",
  "version": "1.0.0",
  "private": true,
  "main": "./public/index.html",
  "homepage": "./",
  "scripts": {
    "build:vendors": "webpack --mode production",
    "build:shared": "webpack --mode production",
    "build:app": "webpack --mode production",
    "build:vendors:dev": "webpack --mode development --progress --colors",
    "build:shared:dev": "webpack --mode development --progress --colors",
    "build:app:dev": "webpack --mode development --progress --colors",
    "build": "run-s lint build:vendors build:shared build:app",
    "clean": "rimraf build",
    "prepare": "npm run clean",
    "lint": "tslint --project tsconfig.json",
    "start": "run-s build:vendors:dev build:shared:dev webpackdevServer:dev",
    "webpackdevServer:dev": "webpack-dev-server --mode development --hot --inline --progress --colors --content-base ./public",
    "test": "jest"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "../../../testSetup.js",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)(spec|test).(ts)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.3",
    "@types/jsonpath": "^0.2.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.4",
    "@types/request": "^2.47.0",
    "babel-loader": "^7.1.4",
    "babel-preset-react-app": "^3.1.1",
    "coveralls": "^3.0.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.11",
    "hard-source-webpack-plugin": "^0.6.9",
    "jest": "^23.0.0",
    "jest-enzyme": "^6.0.0",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react-app-rewired": "^1.4.1",
    "react-dev-utils": "^5.0.0",
    "react-hot-loader": "^4.1.3",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-loader": "^3.6.0",
    "typings-for-css-modules-loader": "^1.7.0",
    "typescript": "2.7.2",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {

    "@bfemulator/sdk-shared": "^1.0.0",
    "@bfemulator/ui-react": "^1.0.0",
    "@fuselab/ui-fabric": "~1.1.0",
    "@fuselab/ui-shared": "~1.1.0",
    "@uifabric/icons": "^5.6.0",
    "@uifabric/styling": "^5.20.0",
    "@uifabric/utilities": "^5.19.0",
    "base64url": "^2.0.0",
    "botframework-webchat": "0.13.1-master.6da7b6c",
    "@uifabric/merge-styles": "^6.2.0",

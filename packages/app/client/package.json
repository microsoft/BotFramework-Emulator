    "@uifabric/utilities": "^5.19.0",
    "base64url": "^2.0.0",
    "classnames": "^2.2.5",
    "enzyme": "^3.3.0",
    "glamor": "^2.20.40",
    "gulp": "4.0.0",
    "immutable": "^3.8.2",
    "jsonpath": "^1.0.0",
    "lodash": "^4.17.5",
    "office-ui-fabric-react": "^5.43.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
